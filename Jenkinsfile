pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Building Phase of CICD"
            }
        }
        stage('DeployToStaging') {
            steps {
                echo "Deploying to Staging Phase of CICD"
            }
        }
        stage('DeployToProduction') {
            steps {
                echo "Deploying to production Phase of CICD"
            }
        }
    }
}
